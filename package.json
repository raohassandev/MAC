
{
  "name": "macsys-root",
  "version": "1.0.0",
  "description": "Modbus Device Management System - Root Package",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node index.js",
    "server:dev": "nodemon index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "push": "git add . && git commit -m 's' && git push"
  },
  "keywords": [
    "modbus",
    "device",
    "management",
    "iot"
  ],
  "author": "Israr ul Haq",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "modbus-serial": "^8.0.11",
    "mongoose": "^7.3.4"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^3.0.1"
  }
}